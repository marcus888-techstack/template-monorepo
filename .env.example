# Docker Compose Environment Variables
# Copy this file to .env for Docker Compose configuration
# This file is used by docker-compose.yml to configure services

# PostgreSQL Database
POSTGRES_DB=bakery_db
POSTGRES_USER=bakery_user
POSTGRES_PASSWORD=bakery_pass
POSTGRES_PORT=5432

# pgAdmin (Database GUI)
PGADMIN_DEFAULT_EMAIL=admin@bakery.com
PGADMIN_DEFAULT_PASSWORD=admin
PGADMIN_PORT=5050

# Redis Cache
REDIS_PORT=6379

# Application Ports
BACKEND_PORT=5000
FRONTEND_PORT=5173

# Environment (development | production)
NODE_ENV=development
ENVIRONMENT=development